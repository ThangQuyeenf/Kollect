{
    "version": 2,
    "builds": [
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*).(js|css|png|jpe?g)",
        "dest": "/dist/$1.$2"
      },
      {
        "src": "/(.*).ico",
        "headers": { "cache-control": "public, max-age=31536000" },
        "dest": "/dist/$1.ico"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
    ]
  }
  